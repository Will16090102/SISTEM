    <script>
        // Navegación entre pestañas
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', function() {
                const screenId = this.getAttribute('data-screen') + 'Screen';
                
                // Actualizar pestañas activas
                document.querySelectorAll('.nav-tab').forEach(t => t.classList.remove('active'));
                this.classList.add('active');
                
                // Mostrar pantalla correspondiente
                document.querySelectorAll('.screen').forEach(screen => {
                    screen.classList.remove('active');
                });
                document.getElementById(screenId).classList.add('active');
            });
        });

        // Funciones específicas del módulo logístico
        function actualizarSeguimiento() {
            alert('Actualizando posiciones de buques...');
            // En una implementación real, aquí se conectaría con APIs de tracking
        }

        function exportarReporte() {
            alert('Generando reporte de operaciones...');
            // En una implementación real, aquí se generaría un Excel/PDF
        }

        // Efectos visuales
        document.querySelectorAll('.form-input, .form-select, .form-textarea').forEach(input => {
            input.addEventListener('focus', function() {
                this.style.transform = 'scale(1.02)';
            });
            
            input.addEventListener('blur', function() {
                this.style.transform = 'scale(1)';
            });
        });

        // Interacción con las cards de operaciones
        document.querySelectorAll('.operation-card').forEach(card => {
            card.addEventListener('click', function() {
                // Navegar a la pantalla de seguimiento con la operación seleccionada
                document.querySelector('[data-screen="seguimiento"]').click();
            });
        });
    </script>
